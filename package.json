{
  "name": "uncloudy-grapher",
  "version": "0.2.0",
  "description": "a 2.5D graph library to monitor kubernetes nodes and workloads",
  "module": "exnext",
  "main": "src/index.tsx",
  "author": "Jaycol Kim <jaycol.kim@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build:plugin2": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "build:plugin": "webpack --node-env=production",
    "dev2": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "dev:plugin": "webpack -w -c ./webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "e2e": "yarn exec cypress install && yarn exec grafana-e2e run",
    "e2e:update": "yarn exec cypress install && yarn exec grafana-e2e run --update-screenshots",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "build:web": "webpack --env web --env production --progress",
    "dev:web": "webpack serve --env web --env development --hot"
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  },
  "engines": {
    "node": ">=16 <20"
